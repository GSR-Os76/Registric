<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>GSR.Registric</AssemblyName>
    <RootNamespace>GSR.Registric</RootNamespace>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <PackageId>gsr_registric</PackageId>
    <Title>GSR Registric</Title>
    <Version>2.1.1.0</Version>
    <Authors>GSR</Authors>
    <Company>GSR</Company>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Product>Registric</Product>
    <RepositoryUrl>https://github.com/GSR-Os76/Registric</RepositoryUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Reference Include="GSR.Utilic">
      <HintPath>..\..\EnDecic\GSR.Utilic.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <None Update="README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
